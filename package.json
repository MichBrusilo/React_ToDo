{
  "name": "zadanie-1-4",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:html && npm run test:js",
    "test:html": "globstar nu-html-checker *.html",
    "test:js": "jshint *.js",
    "init-project": "npm install && npm install mkdirp && npm-run-all init:*",
    "init:dirs": "mkdirp sass css vendor images js",
    "init:files": "touch README.md index.html sass/style.scss js/script.js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "build": "webpack --env production",
    "build:clean": "rimraf css/*.css",
    "build:sass": "node-sass --output-style compact -o css sass",
    "build:autoprefixer": "autoprefixer-cli css/style.css",
    "build-dev:sass": "node-sass --output-style expanded --source-map true -o css sass",
    "watch": "npm-run-all build -p watch:*",
    "watch:sassprefixer": "onchange scss/*.scss -- npm-run-all sassprefixer:*",
    "sassprefixer:sass": "node-sass --output-style expanded --source-map true -o css scss",
    "sassprefixer:autoprefixer": "autoprefixer-cli css/style.css",
    "watch:browsersync": "browser-sync start --server --files 'css/*.css' '*.html'",
    "start": "webpack-dev-server --hot --env development"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.23.6",
    "css-loader": "^0.28.11",
    "globstar": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jshint": "^2.9.5",
    "npm-run-all": "^4.1.2",
    "nu-html-checker": "^0.1.0",
    "onchange": "^3.3.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "uuid": "^3.2.1"
  }
}
